{
  "expo": {
    "name": "WeConnect-Africa",
    "slug": "WeConnect-Africa",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/weconnect.jpeg",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/weconnect.jpeg",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.weConnect-Africa.in"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.weConnectAfrica"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "a2a09c4a-67db-4a1f-b53a-3c236a4d53e8"
      }
    }
  }
}
